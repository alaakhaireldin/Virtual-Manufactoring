<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
  <title>List of Projects</title>

<!-- CSS -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
  <link rel="stylesheet" href="../../css/style.css">
</head>

<body>
  <div id="app">
    <div class="main-wrapper">
      <div class="navbar-bg"></div>
      <%- include("user-icon-bar") -%>
      <%- include("side-menu") -%>

      <!-- Main Content -->
    <div class="main-content">
    <section class="section">
      <div class="section-header" onchange="loadFile(event)">
        <h1>List of Projects</h1>
      </div>

    <section class="section">
      <% newListItem.forEach(item => { %>
        <div>
        <button type="button" class="collapsible" id="240">
        <a href="https://apod.nasa.gov/apod/image/2202/AuroraPillars_Correia_4898.jpg">
          <%= item %> </a>
        </button>
        <div class="content">
          <div id="container">
            <canvas id="imageView" class="canvasImages" width="100" height="75">

            </canvas>
          </div>
        </div>
        <br></br>
      </div>
        <% }) %>
    </section>

        <section class="section">

        </section>

  <!-- General JS Scripts -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.nicescroll/3.7.6/jquery.nicescroll.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>


  <!-- Template JS File -->
  <script src="../../js/scripts.js"></script>

  <script src="add-projects-on-list.js" > </script>
  <script src="uplaodImage.js"></script>

  <!-- Canvas script -->
  <script type="text/javascript" src="js/draw-points-canvas.js"></script>
  <script src="js/upload-image-list.js">

  </script>
  <!-- importing files -->
  <script type="text/javascript" src="js/add-projects-on-list.js"></script> 
  <script type="text/javascript" src="js/upload-image-list.js"></script> 
  <!-- <script src="change-canvas-bg.js"></script> -->
  <script src="js/collapsable-list-item.js"></script>
</body>
</html>
